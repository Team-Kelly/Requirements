@startuml subway_realtime

actor user
actor server
actor subway as "서울시 지하철 정보"


user -> server : request()
activate server
server -> subway : API 정보 요청
activate server
activate subway
subway --> server
deactivate server
deactivate subway


server -> server : 가공
activate server
server -> server : 가공된 데이터
deactivate server


server --> user : response
deactivate server



@enduml